{
    "presets": ["module:metro-react-native-babel-preset"],
    "plugins": [
      [
        "module-resolver",
        {
        "cwd": "babelrc",
        "root": ["./src"],
        "extensions": [".js", ".ios.js", ".android.js", ".scss", ".sass"],
        "alias": {
           "src": "./src",
           "styles": "./src/styles"
         }
        } 
      ] 
    ]
  }